
{
  "name": "nerdysouth/stripe-bundle",
  "description": "Stripe Bundle for Symfony 7.2",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "php",
    "bundle",
    "symfony",
    "stripe",
    "webhook"
  ],
  "autoload": {
    "psr-4": {
      "NerdySouth\\StripeBundle\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Brandon DuBois",
      "email": "support@nerdysouthinc.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-curl": "*",
    "symfony/framework-bundle": "7.2.*",
    "symfony/event-dispatcher": "7.2.*",
    "symfony/twig-bundle": "^7.2",
    "doctrine/orm": "^3.3",
    "stripe/stripe-php": "^16.6"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
